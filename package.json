{
  "name": "thonycsdev-portifolio-page",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "yarn run services:up && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "services:up": "docker compose -f infra/docker-compose.yml up -d",
    "services:stop": "docker compose -f infra/docker-compose.yml stop",
    "services:down": "docker compose -f infra/docker-compose.yml down",
    "test:watch": "jest --watch",
    "migration:run": "dotenv -e .env -- node-pg-migrate --migrations-dir ./infra/migrations up",
    "migration:create": "node-pg-migrate --migrations-dir ./infra/migrations create",
    "migration:down": "dotenv -e .env -- node-pg-migrate down --migrations-dir ./infra/migrations"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.4.1",
    "@chakra-ui/react": "^2.10.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@tanstack/react-query": "^5.17.19",
    "@vercel/analytics": "^1.1.2",
    "axios": "^1.6.5",
    "framer-motion": "^11.11.4",
    "next": "14.1.0",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.31",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/pg": "^8.11.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
